	<h1>Listado de preguntas</h1>
	<br>
	<table id="preglist">
	<% var i; for (i=0; i < quizes.length; i++) { %>
		<tr>
			<td>
				<a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a>
			</td>
			<td>
				<a href="quizes/<%= quizes[i].id %>/edit"><button id="editar">editar</button></a>
			</td>
			<td>
				<form method="post" action="quizes/<%= quizes[i].id %>?_method=delete">
					<button id="borrar" type="submit" onClick="return confirm('Borrar: <%= quizes[i].pregunta %>');">
						borrar
					</button>
				</form>
			</td>
		</tr>
	<% } %>
	</table>
	<br>
	<a href="/quizes/new"><button type="button">Crear nueva pregunta</button></a>